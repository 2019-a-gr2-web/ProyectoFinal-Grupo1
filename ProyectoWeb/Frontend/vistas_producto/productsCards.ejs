<div class="container p-4">
    <div class="row">
        <div class="col-md-8">
            <div class="row">
                    <% productos.forEach((producto) => { %>
                <div class="col-md-6">
                    <div class="card text-center">
                        <div class="card-body">
                            <h3 class="card-title text-uppercase">
                                    <%=producto.nombreProducto%>
                            </h3>
                            <img src="<%=producto.imagenProducto%>" alt="" width="200px" height="200px">
                            <h4 class="card-text ">
                                    <%=producto.PVP%>
                            </h4>
                            <a class="btn btn-danger" href="/cars/delete/{{COD_AUTO}}">
                                COMPRAR
                            </a>

                        </div>
                    </div>
                </div>
                <% })%>
            </div>

        </div>
    </div>
</div>

</div>