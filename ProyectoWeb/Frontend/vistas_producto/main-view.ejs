
<table>
    <tr>
        <th>Nombre</th>
        <th>Grados</th>
        <th>Caduca</th>
        <th>Precio</th>
        <th>Tipo</th>
        <th>Acciones</th>
    </tr>
    <% productos.forEach((producto)=>{ %>
        <tr>
            <td> <%=producto.idProducto%> </td>
            <td> <%=producto.nombreProducto%> </td>
            <td> <%=producto.descripcion%> </td>
            <td> <%=producto.codigoProducto%> </td>
            <td> <%=producto.PVP%> </td>
            <td>
                <button>Editar</button>
                <button>Eliminar</button>
            </td>
        </tr>
    <% })%>
</table>